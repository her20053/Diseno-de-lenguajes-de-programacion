digraph {
	graph [rankdir=LR]
	A -> B [label=a]
	B -> D [label=a]
	B -> C [label=b]
	B -> F [label=c]
	C -> C [label=b]
	C -> F [label=c]
	D -> E [label=b]
	D -> F [label=c]
	E -> F [label=c]
	F -> G [label=a]
	F -> G [label=b]
	F -> F [label=c]
	G -> G [label=a]
	G -> G [label=b]
	A [shape=circle style=bold]
	Inicio [shape=point]
	Inicio -> A
	F [shape=doublecircle style=bold]
	G [shape=doublecircle style=bold]
}
